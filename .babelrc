{
    "presets":[
        "stage-0",
        "es2017",
        ["es2015", {
			"modules": false
	    }],
		"react",
        "react-optimize"
    ],
    "plugins": [
        "transform-react-constant-elements",
        "transform-react-inline-elements",
        ["transform-runtime", {
            "polyfill": false,
            "regenerator": true
        }]
    ]
}
